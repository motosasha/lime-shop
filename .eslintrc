{
  "parser": "@babel/eslint-parser",
  "extends": ["eslint:recommended", "prettier"],
  "plugins": ["prettier"],
  "globals": {
    "require": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "requireConfigFile": false
  },
  "rules": {
    "prettier/prettier": "error",
    "strict": 0
  }
}
